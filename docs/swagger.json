    "definitions": {
        "dtos.GetAllRolesResponse": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Role"
                    }
                },
                "totalCount": {
                    "type": "integer"
                }
            }
        },
        "dtos.LoginUser": {
            "type": "object",
            "required": [
                "email",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string",
                    "format": "string",
                    "example": "admin@gmail.com"
                },
                "password": {
                    "type": "string",
                    "format": "string",
                    "example": "1234567890"
                }
            }
        },
        "dtos.RegisterUser": {
            "type": "object",
            "required": [
                "avatar_user",
                "email",
                "first_last_name",
                "password"
            ],
            "properties": {
                "avatar_user": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "first_last_name": {
                    "type": "string"
                },
                "password": {
                    "type": "string",
                    "minLength": 8
                }
            }
        },
        "dtos.Response": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "data": {},
                "status": {
                    "type": "string"
                }
            }
        },
        "dtos.RoleInput": {
            "type": "object",
            "required": [
                "role_name"
            ],
            "properties": {
                "role_name": {
                    "type": "string",
                    "format": "string",
                    "example": "admin"
                },
                "role_nivel": {
                    "type": "integer",
                    "format": "int",
                    "example": 1
                }
            }
        },
        "handler.errorResponse": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string"
                }
            }
        },
        "models.Role": {
            "type": "object",
            "required": [
                "role_name"
            ],
            "properties": {
                "created_at": {
                    "type": "string"
                },
                "id_role": {
                    "type": "integer"
                },
                "role_name": {
                    "type": "string"
                },
                "role_nivel": {
                    "type": "integer"
                },
                "status": {
                    "type": "boolean"
                },
                "updated_at": {
                    "type": "string"
                }
            }
        }
    },
    "securityDefinitions": {
        "ApiKeyAuth": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
}